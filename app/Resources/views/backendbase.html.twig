<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" >
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<meta name="keywords" content="Photography, Photo Images, Responsive, Business, Corporate, Gallery, Notebook" />
<meta name="description" content="PhotoBax Responsive Photography Business Template" />
<!-- basehead -->
    <head>
        <meta charset="UTF-8" />
        <!-- title -->
        <title>{% block title %}Page Name{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
{% block stylesheets %}
        <link href="/source/manage/css/bootstrap.min.css" rel="stylesheet">
        <link href="/source/manage/css/plugins/metisMenu/metisMenu.min.css" rel="stylesheet">
        <link href="/source/manage/css/plugins/dataTables.bootstrap.css" rel="stylesheet">
        <link href="/source/manage/css/sb-admin-2.css" rel="stylesheet">
        <link href="/source/change/css/main.css" rel="stylesheet"  />
        <link href="/source/manage/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
        <link rel="stylesheet" href="{{ asset('bundles/wechatapi/css/tipscss.css') }}" />
{% endblock %}

{% block javascripts %}
<script type="text/javascript" src="/source/manage/js/jquery-1.11.0.js"></script>
<script type="text/javascript" src="/source/manage/js/bootstrap.min.js"></script>
<script type="text/javascript" src="/source/manage/js/plugins/metisMenu/metisMenu.min.js"></script>
<script type="text/javascript" src="/source/manage/js/plugins/dataTables/jquery.dataTables.js"></script>
<script type="text/javascript" src="/source/manage/js/plugins/dataTables/dataTables.bootstrap.js"></script>
<script type="text/javascript" src="/source/manage/js/sb-admin-2.js"></script>
<script type="text/javascript" src="/source/change/js/main.js"></script>
<script type="text/javascript" src="{{ asset('bundles/wechatapi/js/tipsbox.js') }}"></script>
{% endblock %}
</head>
<body>
  <div id="wrapper">
<!-- Navigation -->
<!-- Navigation -->
<nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <img src="/source/change/img/adplog.png" style="height:44px"/>
    </div>
    <!-- /.navbar-header -->

    <ul class="nav navbar-top-links navbar-right">

        <li class="dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                <i class="fa fa-user fa-fw"></i>  <i class="fa fa-caret-down"></i>
            </a>
            <ul class="dropdown-menu dropdown-user">
                <li><a href="/user/preference"><i class="fa fa-gear fa-fw"></i> Settings</a>
                </li>
                <li class="divider"></li>
                <li><a href="/user/logout"><i class="fa fa-sign-out fa-fw"></i> Logout</a>
                </li>
            </ul>
            <!-- /.dropdown-user -->
        </li>
        <!-- /.dropdown -->
    </ul>
    <!-- /.navbar-top-links -->

    <div class="navbar-default sidebar" role="navigation">
        <div class="sidebar-nav navbar-collapse">
            <ul class="nav" id="side-menu">

            {% if "wechat_api_wechatmenu" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navmenu %}{% endblock %}"><a href="/wechat/menu"><i class="fa fa-th-list fa-fw"></i> NAVIGATION</a></li>
            {% endif %}
            {% if "article_control" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navpag %}{% endblock %}"><a href="/article/page"><i class="fa fa-columns fa-fw"></i> PAGE</a></li>
            {% endif %}
            {% if "wechat_api_keyword" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navkeyword %}{% endblock %}"><a href="/wechat/keyword"><i class="fa fa-puzzle-piece fa-fw"></i> KEYWORD</a></li>
            {% endif %}
            {% if "wechat_api_reply" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navreplay %}{% endblock %}"><a href="/wechat/reply"><i class="fa fa-tag fa-fw"></i> AUTO-REPLY</a></li>
            {% endif %}
            {% if "wechat_api_groupnews" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navgroupnews %}{% endblock %}"><a href="/wechat/groupnews"><i class="fa fa-newspaper-o fa-fw"></i> Group News</a></li>
            {% endif %}
            {% if "wechat_api_jssdk" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navjssdk %}{% endblock %}"><a href="/wechat/jssdk"><i class="fa fa-unlock fa-fw"></i> Jssdk Build</a></li>
            {% endif %}
            {% if "wechat_api_qrcode" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navqrcode %}{% endblock %}"><a href="/wechat/qrcode"><i class="fa fa-qrcode fa-fw"></i> Qrcode</a></li>
            {% endif %}
            {% if "wechat_api_oauth" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navoauth %}{% endblock %}"><a href="/wechat/oauth"><i class="fa fa-universal-access fa-fw"></i> Oauth</a></li>
            {% endif %}
            {% if "wechat_api_stores" in userinfo.permission or "user_usercontrol" in userinfo.permission %}
                   <li class="{% block navstores %}{% endblock %}"><a href="/wechat/stores"><i class="fa fa-shopping-cart fa-fw"></i> Stores</a></li>
            {% endif %}
            </ul>
        </div>
        <!-- /.sidebar-collapse -->
    </div>
    <!-- /.navbar-static-side -->
</nav>
<!-- Navigation end-->
    <div id="maincontent">
{% block maincontent %}
{% endblock %}
    </div>
  </div>
  <!-- warningpopup -->
<div id="warningpopup">

</div>
<!-- warningpopup end -->
<!-- comfirmpopup -->
<div id="comfirmpopup">
  <div class="comfirmpopup">
  </div>
</div>
<!-- comfirmpopup end -->
<div id="loadingpopup">
  <div class="loadingpopup">
    <i class="fa fa-spinner fa-spin"></i>
  </div>
</div>

<!-- progress -->
<div id="myprogress">
  <div class="progress progress-striped active">
     <div class="progress-bar progress-bar-success" role="progressbar"
        aria-valuenow="10" aria-valuemin="0" aria-valuemax="100"
        style="width: 100%;">
        <span class="sr-only">40% 完成</span>
     </div>
  </div>
</div>
<!-- progress end -->
</body>
</html>
